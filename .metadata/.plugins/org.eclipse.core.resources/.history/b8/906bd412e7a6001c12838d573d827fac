package model.world;
import java.awt.*;
public class Cover {
  private int currentHP;
  private  Point location;
  private int x;
  private int y;

public int getCurrentHP(){
    return this.currentHP;
}
public void setCurrentHP(int x){
    this.currentHP=x;   
}
public Point getLoctaion(){
    return this.location;
}
Cover(int x ,int y){
this.x=x;
this.y=y;
int r = (int)Math.floor(Math.random()*(999-100+1)+100);
setCurrentHP(r);
}
}
